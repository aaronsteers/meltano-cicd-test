            "type": "string",
            "examples": [
                "project_id",
                "send_anonymous_usage_stats"
            ]
        },
        "changed_from": {
            "description": "The prior value.",
            "type": ["string", "boolean", "null"]
        },
        "changed_to": {
            "description": "The new value.",
            "type": ["string", "boolean", "null"]
        }
    },
    "required": [
        "setting_name",
        "changed_from",
        "changed_to"
